                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT Thuoc.TenThuoc, DonVi.TenDonVi, Thuoc.SoLuong, CachDung.SoLanDung, PhieuKhamBenh.NgayKham, MONTH(PhieuKhamBenh.NgayKham) AS Thang
FROM     CachDung INNER JOIN
                  CT_PhieuKhamBenh ON CachDung.ID_CachDung = CT_PhieuKhamBenh.ID_CachDung INNER JOIN
                  PhieuKhamBenh ON CT_PhieuKhamBenh.ID_PhieuKham = PhieuKhamBenh.ID_PhieuKham INNER JOIN
                  Thuoc ON CT_PhieuKhamBenh.ID_Thuoc = Thuoc.ID_Thuoc INNER JOIN
                  DonVi ON Thuoc.ID_DonVi = DonVi.ID_DonVi
WHERE  (MONTH(PhieuKhamBenh.NgayKham) = @thang)
ORDER BY Thang</CommandText>
